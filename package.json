{
    "name": "cli-tools",
    "version": "5.0.0",
    "description": "",
    "main": "index.js",
    "type": "module",
    "packageManager": "pnpm@9.1.4",
    "scripts": {
        "dev": "npm run dev:cli",
        "dev:ui": "pnpm --filter \"ui\" dev",
        "dev:cli": "cross-env MODE=cliTest pnpm --filter \"cli\" dev",
        "dev:web": "cross-env MODE=web pnpm --filter \"cli\" dev",
        "mon:web": "nodemon packages/server/dist/web.js --watch packages/server/dist",
        "build": "npm run build:cli",
        "build:cli": "cross-env MODE=cli pnpm --filter \"cli\" build",
        "build:web": "cross-env MODE=web pnpm --filter \"cli\" build",
        "build:ui": "pnpm --filter \"ui\" build-only",
        "build:web-all": "npm run build:web && npm run build:ui"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "@linzb93/utils": "^0.0.14",
        "axios": "1.6.8",
        "chalk": "^5.3.0",
        "cron": "^3.1.7",
        "dayjs": "^1.9.6",
        "diff": "^7.0.0",
        "execa": "^6.0.0",
        "fs-extra": "^11.3.0",
        "lodash-es": "^4.17.21",
        "nodemon": "^3.1.4",
        "p-map": "^5.3.0",
        "puppeteer": "^24.8.2",
        "sass": "^1.74.1",
        "socket.io": "^4.7.5"
    },
    "devDependencies": {
        "@types/diff": "^7.0.2",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.14.15",
        "cross-env": "^7.0.3",
        "husky": "^9.0.11",
        "json-schema-to-typescript": "^15.0.0",
        "typescript": "~5.4.0",
        "vite": "^5.3.4",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0"
    }
}
